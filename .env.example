# Environment
NODE_ENV=development


# Protocol: ftp | ftps | sftp
PROTOCOL=sftp

# Server credentials
HOST=your.sftp.server
USER=username
PASSWORD=password

# Base remote path for operations (used as root)
ROOT_PATH=/remote/base/path



# Optional: Proxy settings (SFTP only). Leave disabled if not using a proxy.
PROXY_ENABLED=false

# One of: socks5 | socks4 | http | https
PROXY_TYPE=https

# Proxy endpoint
PROXY_HOST=proxy.example.com
PROXY_PORT=9443

# Proxy authentication (optional)
PROXY_USERNAME=
PROXY_PASSWORD=

PROXY_DEDICATED_IP=
PROXY_LOCATION=
PROXY_SERVICE_TYPE=

# TLS options
# For TLS-wrapped SOCKS or HTTPS proxies with custom certificates
PROXY_SECURE=true
PROXY_SECURE_REJECT_UNAUTHORIZED=false
